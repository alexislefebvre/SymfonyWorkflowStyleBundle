{
  "name" : "alexislefebvre/symfony-workflow-style",
  "type" : "symfony-bundle",
  "description" : "Symfony bundle adding style to Symfony workflow dumps",
  "keywords" : ["symfony", "twitter", "reader", "bundle"],
  "homepage": "https://github.com/alexislefebvre/symfony-workflow-style",
  "license" : "MIT",
  "authors" : [{
    "name" : "Alexis Lefebvre",
    "email" : "alexislefebvre@gmail.com",
    "homepage": "https://alexislefebvre.com/",
    "role": "Developer"
  }],
  "require" : {
    "php": ">=7.1",
    "symfony/config": "^4.1",
    "symfony/console": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/http-kernel": "^4.1",
    "symfony/workflow": "^4.1",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.6|~7.0"
  },
  "conflict" : {
    "symfony/framework-bundle": "<4.1.8"
  },
  "autoload" : {
    "psr-4" : {
      "AlexisLefebvre\\Bundle\\SymfonyWorflowStyleBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Acme\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
